[api]
log_level=CRITICAL
user=<%= @access_user %>
password=<%= @access_password %>
tenant=<%= @access_tenant %>
keystone_endpoints=http://<%= scope.lookupvar('zabbix::params::keystone_vip') %>:5000/v2.0
nova_os_map=v2/%(tenant_id)s/flavors
glance_map=v1/images
keystone_service_map=v2.0/tenants
keystone_map=v2.0/tenants
cinder_map=v1/%(tenant_id)s/volumes
nova_os_timeout=5
glance_timeout=5
keystone_service_timeout=5
keystone_timeout=5
cinder_timeout=5
