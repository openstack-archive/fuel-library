### Managed by Puppet ###

# Fake server declaration to enforce running agent without
# separate server declaration in /etc/zabbix/zabbix_agent.d/server.conf
Server=<%= @server_ip %>
ServerActive=<%= @server_ip %>

# Server port for sending active checks
#ServerPort=<%= @server_listen_port %>

Hostname=<%= @fqdn %>

ListenPort=<%= @agent_listen_port %>

#ListenIP=<%= @agent_listen_ip %>

#SourceIP=<%= @agent_source_ip %>

StartAgents=5

#RefreshActiveChecks=120

#DisableActive=1

#EnableRemoteCommands=1

DebugLevel=3

PidFile=<%= @agent_pid_file %>

LogFile=<%= @agent_log_file %>

#LogFileSize=1

Timeout=10

Include=<%= @agent_include %>

