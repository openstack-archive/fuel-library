# Start logstash
START=true

# Java options
LS_JAVA_OPTS="${LS_JAVA_OPTS} -Xms<%= scope.lookupvar('logstash::heap_size') %> -Xmx<%= scope.lookupvar('logstash::heap_size') %> -Des.path.home=/x/elasticsearch"
JAVA_OPTS="${JAVA_OPTS} ${LS_JAVA_OPTS}"

# Open files limit
#OPEN_FILES=2049

# Nice setting
#NICE=19

# Number of filter threads ( default 1 )
#FILTER_THREADS=1
