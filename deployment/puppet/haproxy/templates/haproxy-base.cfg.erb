global
<% @global_options.sort.each do |key,val| -%>
<% if val.is_a?(Array) -%>
<% val.each do |item| -%>
  <%= key %>  <%= item %>
<% end -%>
<% else -%>
  <%= key %>  <%= val %>
<% end -%>
<% end -%>

defaults
<% @defaults_options.sort.each do |key,val| -%>
<% if val.is_a?(Array) -%>
<% val.each do |item| -%>
  <%= key %>  <%= item %>
<% end -%>
<% else -%>
  <%= key %>  <%= val %>
<% end -%>
<% end -%>
<% if @custom_fragment -%>

<%= @custom_fragment %>
<% end -%>
