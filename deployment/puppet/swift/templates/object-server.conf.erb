[DEFAULT]
devices = <%= devices %>
bind_ip = <%= storage_local_net_ip %>
bind_port = <%= bind_port %>
mount_check = <%= mount_check %>
user = <%= user %>
log_facility = LOG_LOCAL2
workers = <%= workers %>

[pipeline:main]
pipeline = <%= pipeline.to_a.join(' ') %>

[app:object-server]
use = egg:swift#object

[object-replicator]
concurrency = <%= replicator_concurrency %>

[object-updater]
concurrency = <%= updater_concurrency %>

[object-auditor]
