broker_host: '<%= @rabbitmq_host %>'
broker_port: 5672
broker_username: '<%= @rabbitmq_astute_user %>'
broker_password: '<%= @rabbitmq_astute_password %>'
broker_queue: 'naily'
broker_publisher_queue: 'nailgun'
broker_exchange: 'nailgun'
iops: <%= @iops %>
bootstrap_profile: '<%= @bootstrap_profile %>'
