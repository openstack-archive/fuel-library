[Unit]
Name=oswl vm collector daemon

[Service]
Restart=on-failure
RestartSec=10s
StartLimitBurst=10
TimeoutStartSec=10s
TimeoutStopSec=5s

StandardInput=null
StandardOutput=syslog
StandardError=syslog+console
SyslogIdentifier=oswl_vm_collectord

ExecStart=/usr/bin/oswl_collectord vm

[Install]
WantedBy=multi-user.target

